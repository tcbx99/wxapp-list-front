<!--pages/mission-modify/success.wxml-->
<view class="page">
    <view class="weui-msg">
        <view class="weui-msg__icon-area">
            <icon type="success" size="93"></icon>
        </view>
        <view class="weui-msg__text-area">
            <view class="weui-msg__title">创建成功</view>
            <view class="weui-msg__desc">任务名称：{{name}}</view>
            <view class="weui-msg__desc">任务执行者：</view>
            <view class="weui-msg__desc">{{nameArray}}</view>
        </view>
        <view class="weui-msg__opr-area">
            <view class="weui-btn-area">
                <button class="weui-btn" type="primary">发送链接到群</button>
                <button class="weui-btn" type="default">返回主界面</button>
            </view>
        </view>
    </view>
</view>
